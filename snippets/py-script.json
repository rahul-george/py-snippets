{
    "for-loop-list": {
        "prefix": "for/list",
        "body": ["for ${1:item} in ${2:list}:"],
        "description": "Iterate over a list"
    },
    "for-else-loop": {
        "prefix": "for/else/list",
        "body": ["for ${1:item} in ${2:list}:", "\t${3:statement}", "\tbreak", "else:", "\t${4:statement}"],
        "description": "Iterate over a list"
    },
    "if-else": {
        "prefix": "if/else",
        "body": ["if ${1:condition}:", "\t${2:statement}", "else:", "\t${3:statement}"],
        "description": "Code snippet for if-else condition"
    },
    "elif": {
        "prefix": "elif",
        "body": ["elif ${1:condition}:", "\t${2:pass}"],
        "description": "Code snippet for an elif block"
    },
    "if(main)": {
        "prefix": "__main__",
        "body": ["if __name__ == \"__main__\":", "    ${1:pass}"],
        "description": "`if __name__ == \"__main__\": ...` block"
    }
}